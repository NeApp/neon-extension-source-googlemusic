{
  "name": "Google Play Music",
  "version": "0.1.0",

  "content_scripts": [
    {
      "matches": [
        "https://play.google.com/music/*"
      ],
      "js": [
        "source.googlemusic/source.googlemusic.js"
      ]
    }
  ],

  "permissions": {
    "origins": [
      "*://play.google.com/music/*"
    ]
  }
}