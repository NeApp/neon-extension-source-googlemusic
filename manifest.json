{
  "name": "Google Play Music",
  "version": "1.1.0-beta",
  "content_scripts": [
    {
      "id": "source.googlemusic/main",
      "conditions": [
        {
          "pattern": "*://play.google.com/music/*",
          "pageUrl": {
            "hostEquals": "play.google.com",
            "pathPrefix": "/music"
          }
        }
      ],
      "js": [
        "common.js",
        "source/common.js",
        "source/googlemusic/googlemusic.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "source/googlemusic/shim/shim.js"
  ]
}
