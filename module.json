{
  "title": "Google Play Music",

  "content_scripts": [
    {
      "id": "main",
      "conditions": [
        {
          "pattern": "*://play.google.com/music/*",
          "pageUrl": {
            "hostEquals": "play.google.com",
            "pathPrefix": "/music"
          }
        }
      ],
      "js": [
        "common.js",
        "source/common.js",
        "source/googlemusic/googlemusic.js"
      ]
    }
  ],
  "origins": [
    "*://play.google.com/music/*"
  ],

  "services": [
    "configuration",

    "source/activity",
    "source/library"
  ],
  "web_accessible_resources": [
    "source/googlemusic/shim/shim.js"
  ],

  "webpack": {
    "modules": [
      "shim"
    ]
  }
}
